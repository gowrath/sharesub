<!DOCTYPE html>
<html lang="en">
<!--Head-->
<% include partials/head %>

<body>
  <!--Navbar-->
  <% include partials/nav %>
  <!--Main Content-->
  <!--Hero-->
  <section class="container hero">
    <h1 class="red-text text-darken-1">Welcome to
      <%= title %>
    </h1>
    <p class="grey-text text-darken-2">We're building this out. This is ShareSub. Working out of a Pokedex</p>
  </section>
  <!--Carousel-->
  <section class="container sect">
    <div class="carousel carousel-slider center">
      <div class="carousel-item black white-text" id="carousel-1">
      </div>
      <div class="carousel-item black white-text" id="carousel-2">
      </div>
      <div class="carousel-item black white-text" id="carousel-3">
      </div>
      <div class="carousel-item black white-text" id="carousel-4">
      </div>
      <div class="carousel-item black white-text" id="carousel-5">
      </div>
      <div class="carousel-item black white-text" id="carousel-6">
      </div>
      <div class="carousel-item black white-text" id="carousel-7">
      </div>
      <div class="carousel-item black white-text" id="carousel-8">
      </div>
      <div class="carousel-item black white-text" id="carousel-9">
      </div>
    </div>
  </section>
  <!--Cards-->
  <section id="cards" class="container sect">
    <div class="row">
      <div class="col s12 m6 l6 xl4">
        <div class="card yellow darken-4">
          <div class="card-content white-text">
            <span class="card-title">Pok√©mon Data</span>
            <p>Poke Data</p>
          </div>
          <div class="card-action">
            <a class="btn-floating btn-large waves-effect waves-light red darken-2"><i
                class="material-icons">local_offer</i></a>
          </div>
        </div>
      </div>
      <div class="col s12 m6 l6 xl4">
        <div class="card amber darken-4">
          <div class="card-content white-text">
            <span class="card-title">Items Data</span>
            <p>Items Data</p>
          </div>
          <div class="card-action">
            <a class="btn-floating btn-large waves-effect waves-light red darken-2"><i
                class="material-icons">local_offer</i></a>
          </div>
        </div>
      </div>
      <div class="col s12 m6 l6 xl4">
        <div class="card orange darken-4">
          <div class="card-content white-text">
            <span class="card-title">Utility Calculator</span>
            <p>Utility Calculator</p>
          </div>
          <div class="card-action">
            <a class="btn-floating btn-large waves-effect waves-light red darken-2"><i
                class="material-icons">local_offer</i></a>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!--Footer-->
  <% include partials/footer %>
  <!--Scripts-->
  <% include partials/scripts %>
  <!--Custom Scripts-->
  <script src="/javascripts/index.js"></script>
</body>

</html>
